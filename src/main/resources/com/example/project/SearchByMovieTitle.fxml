<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.SearchByMovieTitleController">
   <children>
      <TextField fx:id="titlename" layoutX="45.0" layoutY="57.0" prefHeight="25.0" prefWidth="233.0" promptText="title" />
      <Label layoutX="45.0" layoutY="33.0" text="Input Movie Title" />
      <Button layoutX="284.0" layoutY="57.0" mnemonicParsing="false" onAction="#OnSearchClick" text="Search!" />
      <TextArea fx:id="output" layoutX="45.0" layoutY="96.0" prefHeight="352.0" prefWidth="297.0" />
      <Button layoutX="168.0" layoutY="461.0" mnemonicParsing="false" onAction="#OnGoBackClick" text="Go Back" />
   </children>
</AnchorPane>
